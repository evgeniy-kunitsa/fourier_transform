$("#direct")
  .html("<%= escape_javascript(
    render  partial: "result",
            locals: {
              mode: "direct",
              intervals: @intervals,
              values: @result[:direct]
            }
  )%>");
$("#intermed")
    .html("<%= escape_javascript(
    render  partial: "intermed",
            locals: {
              mode: "intermed",
              intervals: @intervals,
              values: @result[:intermed]
            }
  )%>");
$("#reverse")
  .html("<%= escape_javascript(
    render  partial: "result",
            locals: {
              mode: "reverse",
              intervals: @intervals,
              values: @result[:reverse]
            }
  )%>");
$("#original_function")
  .html("<%= escape_javascript(
    render  partial: "common/plot",
            locals: {
              plot_title: "Original function",
              plot_id: "original_function_plot",
              chart_type: "line",
              values: @original_plot_points
            }
  )%>");
